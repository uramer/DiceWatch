<?xml version="1.0" encoding="utf-8"?>
<manifest
    package="com.example.Dice"
    version="1.0.0"
    api-version="4"
    xmlns="http://tizen.org/ns/packages">
    <profile name="wearable" />
    <ui-application
        appid="com.example.Dice"
        exec="Dice.dll"
        multiple="false"
        nodisplay="false"
        taskmanage="true"
        type="dotnet"
        launch_mode="single">
        <label>Dice</label>
        <icon>Dice.png</icon>
        <metadata
            key="http://tizen.org/metadata/prefer_dotnet_aot"
            value="true" />
        <splash-screens>
          <splash-screen indicator-display="false" orientation="portrait" src="SplashScreen.png" type="img" />
        </splash-screens>
    </ui-application>
    <privileges>
        <privilege>http://tizen.org/privilege/haptic</privilege>
    </privileges>
    <shortcut-list />
    <dependencies />
    <provides-appdefined-privileges />
</manifest>
